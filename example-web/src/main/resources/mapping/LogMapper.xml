<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wholesmart.mapper.LogMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.wholesmart.entity.Log">
		<id column="id" property="id" />
		<result column="user_id" property="userId" />
		<result column="relname" property="relname" />
		<result column="username" property="username" />
		<result column="opera_time" property="operaTime" />
		<result column="opera_stlye" property="operaStlye" />
		<result column="opera_record" property="operaRecord" />
		<result column="opera_result" property="operaResult" />
		<result column="user_ip" property="userIp" />
		<result column="spare0" property="spare0" />
		<result column="spare1" property="spare1" />
		<result column="spare2" property="spare2" />
		<result column="spare3" property="spare3" />
		<result column="spare4" property="spare4" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id AS userId, relname, username, opera_time AS operaTime, opera_stlye AS operaStlye, opera_record AS operaRecord, opera_result AS operaResult, user_ip AS userIp, spare0, spare1, spare2, spare3, spare4
    </sql>

</mapper>
